<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!-- *** Application Parameters *** -->

    <env-entry>
        <env-entry-name>application-name</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>PPM</env-entry-value>
    </env-entry>

    <!-- *** Database Parameters *** -->

    <env-entry>
        <env-entry-name>db-user</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>db-password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>db-url</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>

    <!-- *** Network Parameters *** -->

    <env-entry>
        <env-entry-name>nk-db-user</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>nk-db-password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>nk-db-url</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>

    <!-- *** PayPal Parameters *** -->

    <env-entry>
        <env-entry-name>pp-token</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>pp-url</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>pp-email</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>

    <!-- *** RabbitMQ Parameters *** -->

    <env-entry>
        <env-entry-name>rabbitMQ-host</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>rabbitMQ-user</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>rabbitMQ-password</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>

    <!-- *** Logger Parameters *** -->

    <env-entry>
        <env-entry-name>rabbitMQ-exchange-logger</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>rabbitMQ-routingKey-logger</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value></env-entry-value>
    </env-entry>


</web-app>
